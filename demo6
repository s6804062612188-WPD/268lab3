#include<stdio.h>

int main() {
	int score;
	scanf("%d", &score);
	if (score < 0) {
		printf("error, lower bound");
	} else if (score > 100) {
		printf("error, upper bound");
	} else
		if (score >= 85) {
			printf("A");
		} else if (score >= 75){
			printf("B");
		} else if (score >= 68){
			printf("C\n");
			printf("hmm, you're %d scores short of getting a B.", 75-score);
		} else if (score >= 55){
			printf("D");
		} else {
			printf("F");
		}
		printf("\n");
}
